<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Report</title>
</head>
<body>
    <div class="container">
    <% items.forEach(function(item){ %>
        <div class="card rounded-3xl px-4 py-8 p-lg-10 mb-6">
            <div class="card-header">
                <a href="<%= item.url %>"><h4 class="my-0 fw-normal"><%= item.name %></h4></a>
                <h3><%= item.price %> Ft</h3>
                <p class="text-muted""><%= item.found %></p>
            </div>
            <div class="card-body">
                <img src="<%= item.image %>" alt="Image not avaiable">
            </div>
        </div>
        <hr>
    <% }); %>
    </div>
</body>
</html>